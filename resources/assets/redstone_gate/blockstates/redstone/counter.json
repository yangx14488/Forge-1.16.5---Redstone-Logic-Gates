{
  "multipart": [
    { "when": { "facing": "north" } ,
      "apply": { "model": "redstone_gate:block/redstone/counter", "y": 0 }
    } ,
    { "when": { "facing": "east" } ,
      "apply": { "model": "redstone_gate:block/redstone/counter", "y": 90 }
    } ,
    { "when": { "facing": "south" } ,
      "apply": { "model": "redstone_gate:block/redstone/counter", "y": 180 }
    } ,
    { "when": { "facing": "west" } ,
      "apply": { "model": "redstone_gate:block/redstone/counter", "y": 270 }
    }
  ]
}
